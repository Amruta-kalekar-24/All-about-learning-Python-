class student:
    school = 'WIT'
    def __init__(self,m1,m2,m3):
        self.m1 = m1
        self.m2 = m2
        self.m3 = m3
        
    def avg(self):
        return (self.m1 + self.m2 + self.m3)/3
    
    @classmethod
    def getschool(cls):
        return cls.school
    
    @staticmethod
    def info():
        print("this is student class in abc module")
    
s1 = student(60,70,80)
s2 = student(45,55,75)
print(s1.avg())
print(student.getschool())
student.info()
